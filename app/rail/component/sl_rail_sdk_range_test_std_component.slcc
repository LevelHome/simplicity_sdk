package: "Rail"
description: "Adds Standard phy measurement support to Range Test."
category: "RAIL-SDK|Extension"
quality: "production"
metadata:
  sbom:
    license: Zlib
root_path: "app/rail/component/sl_rail_sdk_range_test_std_component"

requires:
  - name: "device_has_radio"

source:
  - path: "app_measurement_standard.c"
include:
  - path: "."
    file_list:
    - "path": "app_measurement_standard.h"

#-------- Hides Component for user in Studio ---------
ui_hints:
  visibility: never

#-------------- Template Contribution ----------------
template_contribution:
#---------------- Component Catalog ------------------
  - name: component_catalog
    value: range_test_std_component
