id: cmsis_nn
label: CMSIS-NN
package: platform
description: CMSIS-NN neural network kernels for Cortex-M
category: CMSIS|NN
quality: production
metadata:
  sbom:
    license: Apache-2.0
    upstream:
      name: CMSIS-NN
      version: 3f773cbf8f5
      supplier: ARM-software
      url: https://github.com/ARM-software/CMSIS-NN
      package_url: pkg:github/ARM-software/CMSIS-NN@3f773cbf8f5
      comment: CMSIS NN Library as packaged and released by ARM-software, version 4.1.0, tag 23.08, commit hash 3f773cbf8f5
root_path: util/third_party/cmsis
provides:
  - name: cmsis_nn
other_file:
  - path: nn/LICENSE.txt
include:
  - path: nn
  - path: nn/Include
    file_list:
      - path: arm_nn_math_types.h
      - path: arm_nn_tables.h
      - path: arm_nn_types.h
      - path: arm_nnfunctions.h
      - path: arm_nnsupportfunctions.h
      - path: Internal/arm_nn_compiler.h
library:
  - path: lib/gcc/cortex-m33/libcmsis-nn.a
    condition:
      - toolchain_gcc
      - cortexm33
  - path: lib/gcc/cortex-m4/libcmsis-nn.a
    condition:
      - toolchain_gcc
      - cortexm4
  - path: lib/iar/cortex-m33/libcmsis-nn.a
    condition:
      - toolchain_iar
      - cortexm33
