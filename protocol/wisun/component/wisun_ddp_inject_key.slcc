id: wisun_ddp_inject_key
label: DDP inject Wi-SUN device key pair
description: |
  This component provides a Dynamic Data Provisioning (DDP) command for
  injecting a Wi-SUN device key pair.
package: wisun
category: Wi-SUN
quality: evaluation
root_path: protocol/wisun
provides:
  - name: wisun_ddp_inject_key
requires:
  - name: ddp_common
  - name: ddp_inject_key
  - name: emlib_system
  - name: psa_crypto_ecc_secp256r1
include:
  - path: plugin
    file_list:
      - path: sl_wisun_ddp_inject_key.h
source:
  - path: plugin/sl_wisun_ddp_inject_key.c
define:
  - name: SL_WISUN_DDP_INJECT_KEY
    value: 202
template_contribution:
  - name: ddp_command
    value:
      id: SL_WISUN_DDP_INJECT_KEY
      handler: sl_wisun_ddp_inject_key

metadata:
  sbom:
    license: MSLA