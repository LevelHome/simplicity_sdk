id: ddp_nvm_default
label: DDP NVM default interface
description: |
  This component provides a Dynamic Data Provisioning (DDP) command interface
  via NVM.
package: wisun
category: Wi-SUN
quality: evaluation
metadata:
  sbom:
    license: MSLA
ui_hints:
  visibility: never
root_path: protocol/wisun/ddp
provides:
  - name: ddp_nvm
  - name: nvm3_default
    condition: [ddp_common]
requires:
  - name: ddp_common
  - name: nvm3
recommends:
  - id: nvm3_lib
include:
  - path: inc
    file_list:
      - path: nvm3_default.h
source:
  - path: src/nvm3_default.c