id: radio_config_wmbus_collector
label: Custom Radio Configuration for wmbus collector application
package: platform
description: Triggers Radio Configurator UI for setting a proper (Application - Die
  - RF Band) Custom Radio Configurator
category: Platform|Radio Configuration
quality: production
metadata:
  sbom:
    license: Zlib
provides:
- name: radio_config_wmbus_collector
- name: a_radio_config
  allow_multiple: true
ui_hints:
  visibility: never
template_contribution:
- name: component_catalog
  value: radio_config_wmbus_collector
config_file:
- path: platform/radio/radio_configuration/Mbus/wmbus_collector_868/device_generic_family_efr32xg23/rf_band_868/radio_settings.radioconf
  file_id: radio_configuration_settings
  directory: rail
  condition:
  - device_has_radio_subghz
  - device_generic_family_efr32xg23
  unless:
  - hardware_board_rb
  - hardware_board_tb
  - hardware_board_stk
  - device_is_module
  - device_generic_family_efr32xg21
- path: platform/radio/radio_configuration/Mbus/wmbus_collector_868/device_generic_family_efr32xg28/rf_band_868/radio_settings.radioconf
  file_id: radio_configuration_settings
  directory: rail
  condition:
  - device_has_radio_subghz
  - device_generic_family_efr32xg28
  unless:
  - hardware_board_rb
  - hardware_board_tb
  - hardware_board_stk
  - device_is_module
  - device_generic_family_efr32xg21
- path: platform/radio/radio_configuration/Mbus/wmbus_434/device_generic_family_efr32xg23/rf_band_434/radio_settings.radioconf
  file_id: radio_configuration_settings
  directory: rail
  condition:
  - hardware_board_default_rf_band_434
  - device_generic_family_efr32xg23
  unless:
  - device_is_module
  - device_generic_family_efr32xg21
- path: platform/radio/radio_configuration/Mbus/wmbus_collector_868/device_generic_family_efr32xg23/rf_band_868/radio_settings.radioconf
  file_id: radio_configuration_settings
  directory: rail
  condition:
  - hardware_board_default_rf_band_868
  - device_generic_family_efr32xg23
  unless:
  - device_is_module
  - device_generic_family_efr32xg21
- path: platform/radio/radio_configuration/Mbus/wmbus_collector_868/device_generic_family_efr32xg28/rf_band_868/radio_settings.radioconf
  file_id: radio_configuration_settings
  directory: rail
  condition:
  - hardware_board_default_rf_band_868
  - device_generic_family_efr32xg28
  unless:
  - device_is_module
  - device_generic_family_efr32xg21
